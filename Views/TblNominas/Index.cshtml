@model IEnumerable<WebAdmin.Models.TblNomina>

    @{
    ViewData["Title"] = "Nominas";
    Layout = "~/Views/Shared/_Layout.cshtml";
    }
    <style>
        .invoice-box {
            max-width: 800px;
            margin: auto;
            padding: 5px;
            border: 1px solid #eee;

            font-size: 14px;
            line-height: 15px;
            font-family: 'Helvetica Neue', 'Helvetica', Helvetica, Arial, sans-serif;
            color: #555;
        }

        .invoice-box table {
            width: 100%;
            line-height: inherit;
            text-align: left;
            border-collapse: collapse;
        }

        .invoice-box table td {
            padding: 10px;
            vertical-align: top;
        }

        .invoice-box table tr td:nth-child(2) {
            text-align: right;
        }

        .invoice-box table tr.top table td {
            padding-bottom: 10px;
        }

        .invoice-box table tr.top table td.title {
            font-size: 45px;
            line-height: 20px;
            color: #333;
        }

        .invoice-box table tr.information table td {
            padding-bottom: 10px;
        }

        .invoice-box table tr.heading td {
            background: #eee;
            border-bottom: 1px solid #ddd;
            font-weight: bold;
        }

        .invoice-box table tr.details td {
            padding-bottom: 10px;
        }

        .invoice-box table tr.item td {
            border-bottom: 1px solid #eee;
        }

        .invoice-box table tr.item.last td {
            border-bottom: none;
        }

        .invoice-box table tr.total td:nth-child(2) {
            border-top: 2px solid #eee;
            font-weight: bold;
        }
    </style>
    <div class="container-xxl flex-grow-1 container-p-y">
        <h4 class="fw-bold py-3 mb-4"><span class="text-muted fw-light">Administración /</span> Nóminas</h4>
        <div class="card">
            <h5 class="card-header">
                <div class="row">
                    <div class="mb-3 col-md-6">
                        <a id="iNuevo" type="submit" class="btn btn-outline-primary" asp-action="Create"><i
                                class="fas fa-plus"></i> Nuevo</a>
                    </div>
                    <div class="mb-3 col-md-6">
                        <div class="row mb-3" id="iCorpCent" hidden>
                            <label class="col-sm-2 col-form-label" for="basic-icon-default-fullname">Corporativo /
                                Centro</label>
                            <div class="col-sm-10">
                                <div class="input-group input-group-merge">
                                    <span id="basic-icon-default-fullname2" class="input-group-text"><i
                                            class="bx bx-check-shield"></i></span>
                                    <select class="form-select" id="drpCorpCent"
                                        asp-items="@(new SelectList(ViewBag.ListaCorpCent,"IdCentro","CentroDesc"))">
                                        <option value="">SELECCIONAR</option>
                                    </select>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </h5>
            <div class="table-responsive text-nowrap">
                <div class="row">
                    <table class="table" id="tblNominas">

                        <thead>
                            <tr>

                                <th style="text-align:center" hidden>
                                    @Html.DisplayNameFor(model => model.IdUCorporativoCentro)
                                </th>
                                <th style="text-align:center">
                                    @Html.DisplayNameFor(model => model.UsuarioAsignado)
                                </th>
                                <th style="text-align:center">
                                    @Html.DisplayNameFor(model => model.UsuarioRemuneracion)
                                </th>
                                <th style="text-align:center">
                                    @Html.DisplayNameFor(model => model.TipoPagoDesc)
                                </th>
                                <th style="text-align:center">
                                    @Html.DisplayNameFor(model => model.FechaRegistro)
                                </th>
                                <th style="text-align:center">
                                    @Html.DisplayNameFor(model => model.IdEstatusRegistro)
                                </th>
                                <th style="text-align:center">Acciones</th>
                            </tr>
                        </thead>
                        <tbody>
                            @foreach (var item in Model)
                            {
                            <tr>
                                <td style="text-align:center" hidden>
                                    @Html.DisplayFor(modelItem => item.IdUCorporativoCentro)
                                </td>
                                <td style="text-align:center">
                                    @Html.DisplayFor(modelItem => item.UsuarioAsignado)
                                </td>
                                <td style="text-align:center">
                                    @Html.DisplayFor(modelItem => item.UsuarioRemuneracion)
                                </td>
                                <td style="text-align:center">
                                    @Html.DisplayFor(modelItem => item.TipoPagoDesc)
                                </td>
                                <td style="text-align:center">
                                    @Html.DisplayFor(modelItem => item.FechaRegistro)
                                </td>
                                <td style="text-align:center">
                                    @if (item.IdEstatusRegistro == 1)
                                    {
                                    <i class="fas fa-check-circle" style="font-size: 1em; color: green;"></i>
                                    }
                                    else
                                    {
                                    <i class="fas fa-stop-circle" style="font-size: 1em; color: red;"></i>
                                    }
                                </td>
                                <td style="text-align:center">
                                    <a class="btn btn-outline-secondary" title="Imprimir" id="btnPrint"><i
                                            class="fa-solid fa-print" style="font-size: 1em; color: #67C5C4 ;"></i></a>
                                    <a asp-action="Edit" asp-route-id="@item.IdNomina" class="btn btn-outline-secondary"
                                        title="Editar"><i class="far fa-edit"
                                            style="font-size: 1em; color: #FFC300;"></i></a>
                                    <a asp-action="Details" asp-route-id="@item.IdNomina"
                                        class="btn btn-outline-secondary" title="Detalles"><i class="far fa-list-alt"
                                            style="font-size: 1em; color: #67C5C4 ;"></i></a>
                                    <a asp-action="Delete" asp-route-id="@item.IdNomina"
                                        class="btn btn-outline-secondary" title="Desactivar"><i
                                            class="far fa-stop-circle" style="font-size: 1em; color: #FF9333;"></i></a>
                                </td>
                            </tr>
                            }
                        </tbody>
                        <tfoot>
                            <tr>
                                <th style="text-align:center" hidden>
                                    @Html.DisplayNameFor(model => model.IdUCorporativoCentro)
                                </th>
                                <th style="text-align:center" hidden>
                                    @Html.DisplayNameFor(model => model.UsuarioAsignado)
                                </th>
                                <th style="text-align:center">
                                    @Html.DisplayNameFor(model => model.NominaDesc)
                                </th>
                                <th style="text-align:center">
                                    @Html.DisplayNameFor(model => model.UsuarioRemuneracion)
                                </th>
                                <th style="text-align:center">
                                    @Html.DisplayNameFor(model => model.TipoContratacionDesc)
                                </th>
                                <th style="text-align:center">
                                    @Html.DisplayNameFor(model => model.TipoPagoDesc)
                                </th>
                                <th style="text-align:center">
                                    @Html.DisplayNameFor(model => model.FechaRegistro)
                                </th>

                            </tr>
                        </tfoot>
                    </table>
                </div>
            </div>
        </div>
