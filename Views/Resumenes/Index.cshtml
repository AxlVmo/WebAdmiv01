  function DatosPresupuesto() {
    $.ajax({
      type: "GET",
      url: "/TblCentros/DatosPresupuesto/",
      data: {},
      contentType: "application/json;charset=utf-8",
      dataType: "json",
      success: function (list) {

       if (list.length == 0) {
          document.getElementById("i_presupuesto_preventivo").value = null;
          document.getElementById("i_presupuesto_minimo").value = null;
        }
        else {

          for (var i = 0, len = list.length; i < len; i++) {
            
            var presupuesto_minimo = list[i].fMontos;
            var presupuesto_preventivo = list[i].fMontos * 3;
            document.getElementById("i_presupuesto_minimo").innerText = 'Minimo: ' + presupuesto_minimo.toLocaleString('en-US', {style: 'currency',currency: 'USD',});
            document.getElementById("i_presupuesto_preventivo").innerText = 'Preventivo: ' + presupuesto_preventivo.toLocaleString('en-US', {style: 'currency',currency: 'USD',});
          }

        }
      },
      error: function () {
        var v_fMontosPresupuesto = 0;
        document.getElementById('i_presupuesto_preventivo').innerText = 'Preventivo: ' + v_fMontosPresupuesto;
        document.getElementById("i_presupuesto_minimo").innerText = 'Minimo: ' + v_fMontosPresupuesto.toLocaleString('en-US', {style: 'currency',currency: 'USD',});
      }
    });

    return false;
  }
